HISTFILE=$HOME/.zsh_history
HISTSIZE=10000000
KEYTIMEOUT=1

export ZSH=$HOME/.zsh

plugins=(
    git
    jump
    osx
    fzf
)
ZSH_THEME="wcharczuk"

source $ZSH/zsh_init.sh
source $HOME/.zprofile

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/wcharczuk/workspace/google-cloud-sdk/path.zsh.inc' ]; then . '/home/wcharczuk/workspace/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/wcharczuk/workspace/google-cloud-sdk/completion.zsh.inc' ]; then . '/home/wcharczuk/workspace/google-cloud-sdk/completion.zsh.inc'; fi
