HISTFILE=$HOME/.zsh-history
KEYTIMEOUT=1

export ZSH=$HOME/.oh-my-zsh

plugins=(
	git 
	jump 
	osx
)
ZSH_THEME="wcharczuk"

# User configuration
source $ZSH/oh-my-zsh.sh
source $HOME/.zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
source $HOME/.zprofile


[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/wcharczuk/workspace/google-cloud-sdk/path.zsh.inc' ]; then source '/Users/wcharczuk/workspace/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/wcharczuk/workspace/google-cloud-sdk/completion.zsh.inc' ]; then source '/Users/wcharczuk/workspace/google-cloud-sdk/completion.zsh.inc'; fi
